    {
      "name": "A2A News Agent",
      "item": [
        {
          "name": "Fetch Latest Headlines",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"jsonrpc\": \"2.0\",\n  \"id\": \"req-001\",\n  \"method\": \"message/send\",\n  \"params\": {\n    \"message\": {\n      \"kind\": \"message\",\n      \"role\": \"user\",\n      \"parts\": [\n        {\n          \"kind\": \"text\",\n          \"text\": \"fetch latest headlines\"\n        }\n      ],\n      \"messageId\": \"msg-001\"\n    }\n  }\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "http://localhost:5001/a2a/news",
              "protocol": "http",
              "host": ["localhost"],
              "port": "5001",
              "path": ["a2a", "news"]
